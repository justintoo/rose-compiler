#!/usr/bin/env bash

#--------------------------------------------------------------------
# Global Variables
#--------------------------------------------------------------------
ROSE_SH_HOME="$(cd "$(dirname "$0")/.." && pwd)"
ROSE_SH_BIN_HOME="${ROSE_SH_HOME}/bin"
ROSE_SH_LIB_HOME="${ROSE_SH_HOME}/lib"

#--------------------------------------------------------------------
# Modules
#--------------------------------------------------------------------
source "${ROSE_SH_LIB_HOME}/strict-mode.shinc"
source "${ROSE_SH_LIB_HOME}/rose/rose.shinc"

rose::main ${@:---help}

